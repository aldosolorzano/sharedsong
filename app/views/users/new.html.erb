<h1>Sign up</h1>
<p><%= @user.errors.full_messages.join(', ') %></p>
<%= form_for @user do |u|  %>
  <div class="">
    <%= u.label :first_name%>
    <%= u.text_field :first_name %>
  </div>
  <div class="">
    <%= u.label :last_name%>
    <%= u.text_field :last_name %>
  </div>
  <div class="">
    <%= u.label :email%>
    <%= u.email_field :email %>
  </div>
  <div class="">
    <%= u.label :password%>
    <%= u.password_field :password %>
  </div>
  <div class="">
    <%= u.label :password_confirmation%>
    <%= u.password_field :password_confirmation %>
  </div>
  <%= u.submit %>
<% end %>
